            <action android:name="com.t2.biofeedback.service.spinedata.BROADCAST" />
            </intent-filter>
        </receiver>
        <activity android:name="t2.spine.communication.android.SendBroadcastActivity"></activity>
        <activity android:name=".compassionMeditation.ViewHistoryActivity" android:screenOrientation="portrait"></activity>
        

        

