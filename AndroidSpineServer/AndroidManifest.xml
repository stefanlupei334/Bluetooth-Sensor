<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.t2"
      android:versionCode="1"
      android:versionName="1.1.1">



    <application android:icon="@drawable/icon" android:label="@string/app_name">

        <activity android:name=".AndroidSpineServerMainActivity"
        			android:excludeFromRecents="true"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".biomap.BioDetailActivity" 
        			android:excludeFromRecents="true"
        			android:label="biomap">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




        <activity android:name=".biomap.BioMapActivity" 
        			android:excludeFromRecents="true"
        			android:label="biomap">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="t2.spine.communication.android.AndroidMessageServer">
            <intent-filter>
            <action android:name="com.t2.biofeedback.service.spinedata.BROADCAST" />
            </intent-filter>
        </receiver>
        <activity android:name="t2.spine.communication.android.SendBroadcastActivity"></activity>
        

        

    </application>
</manifest>