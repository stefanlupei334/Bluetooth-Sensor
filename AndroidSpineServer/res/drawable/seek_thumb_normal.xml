<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
    	<shape android:shape="oval">
    		<gradient
    			android:startColor="#FFbc1111"
				android:endColor="#FF551111"
				android:centerY="0.25"
				android:angle="270"/>
			<size
				android:width="35dp"
				android:height="40dp"/>
			<stroke
				android:width="1dp"
				android:color="#FF731111" />
		</shape>
	</item>
	<item android:top="1dp" android:bottom="10dp" android:left="2dp" android:right="2dp">
		<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
			<gradient
    			android:startColor="#DDFFFFFF"
				android:endColor="#00FFFFFF"
				android:angle="270"/>
		</shape>
	</item>
	<item android:top="10dp" android:bottom="2dp" android:left="4dp" android:right="4dp">
		<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
			<gradient
    			android:startColor="#77FFFFFF"
				android:endColor="#00FFFFFF"
				android:angle="90"/>
		</shape>
	</item>
</layer-list>
